{
  "name": "lush-lapa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "lush_lapa/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "lush_lapa"
      },
      "dependsOn": ["@lhg/utils:build"]
    },
    "start:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start:dev",
        "cwd": "lush_lapa"
      }
    }
  },
  "tags": ["app", "backend"]
}
